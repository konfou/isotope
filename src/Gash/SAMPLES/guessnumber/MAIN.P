class Main extends Object
{

	proc Main()
	{
		Integer n
		Asm.New(n)
		Math.getRandomNumber(n)
		Integer.Set(n, n%100)
		String s
		String.New(s)
		Integer guess
		Asm.New(guess)
		Bool keepGoing
		Bool.New(keepGoing)
		Asm.Set(keepGoing, Bool.true)
		Asm.While(keepGoing, Equal To, Bool.true)
		{
			Popups.GetStringBox('Guess a number between 1 and 100', s)
			s.toInteger(guess)
			Asm.If(guess, Equal To, n)
			{
				Popups.MessageBox('Hooray!!!!!!!!!!!!', 'You Got It !!!!!!!')
				Asm.Set(keepGoing, Bool.false)
				Asm.Else()
				Asm.If(guess, Less Than, n)
				{
					Popups.MessageBox('Try Again', 'Too Low')
					Asm.Else()
					Popups.MessageBox('Try Again', 'Too High')
				}
			}
		}
	}

}


