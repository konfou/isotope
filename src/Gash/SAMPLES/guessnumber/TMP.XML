<File>
	<Class Name="Main" Parent="Object">
		<Procedure Name="Main">
			<Instructions>
				<Var Class="Integer" Name="n" />
				<Call Class="Asm" Name="New">
					<Param Exp="n" />
				</Call>
				<Call Class="Asm" Name="Set">
					<Param Exp="n" />
					<Param Exp="6" />
				</Call>
				<Var Class="String" Name="s" />
				<Call Class="String" Name="New">
					<Param Exp="s" />
				</Call>
				<Var Class="Integer" Name="guess" />
				<Call Class="Asm" Name="New">
					<Param Exp="guess" />
				</Call>
				<Var Class="Integer" Name="keepGoing" />
				<Call Class="Asm" Name="New">
					<Param Exp="keepGoing" />
				</Call>
				<Call Class="Asm" Name="Set">
					<Param Exp="keepGoing" />
					<Param Exp="1" />
				</Call>
				<Call Class="Asm" Name="While">
					<Param Exp="keepGoing" />
					<Param Exp="Equal To" />
					<Param Exp="1" />
					<Instructions>
						<Call Class="Popups" Name="GetStringBox">
							<Param Exp="'Guess a number between 1 and 10'" />
							<Param Exp="s" />
						</Call>
						<Call Class="String" Name="toInteger">
							<Param Exp="s" />
							<Param Exp="guess" />
						</Call>
						<Call Class="Asm" Name="If">
							<Param Exp="guess" />
							<Param Exp="Equal To" />
							<Param Exp="n" />
							<Instructions>
								<Call Class="Popups" Name="MessageBox">
									<Param Exp="'Hooray!!!!!!!!!!!!'" />
									<Param Exp="'You Got It !!!!!!!'" />
								</Call>
								<Call Class="Asm" Name="Set">
									<Param Exp="keepGoing" />
									<Param Exp="0" />
								</Call>
								<Call Class="Asm" Name="Else" />
								<Call Class="Asm" Name="If">
									<Param Exp="guess" />
									<Param Exp="Less Than" />
									<Param Exp="n" />
									<Instructions>
										<Call Class="Popups" Name="MessageBox">
											<Param Exp="'Try Again'" />
											<Param Exp="'Too Low'" />
										</Call>
										<Call Class="Asm" Name="Else" />
										<Call Class="Popups" Name="MessageBox">
											<Param Exp="'Try Again'" />
											<Param Exp="'Too High'" />
										</Call>
									</Instructions>
								</Call>
							</Instructions>
						</Call>
					</Instructions>
				</Call>
			</Instructions>
		</Procedure>
	</Class>
</File>
